<%- include __header %>
 

    <form action="/addcroma" method="POST">
    
    <fieldset>
     <legend>Agrega un Croma</legend>
      Nombre de Usuario:<br>
      <input type="text" name="usuario">
      <br>
      Nombre del Croma:<br>      
         <select name="nombre">
          <% kroma.forEach((kromas)=>{%>
        <option value="<%=kromas.nombre%>"><%=kromas.nombre%> </option>
        <%})%>
      </select>  
          
     
      <input type="submit" value="Aceptar">
    </fieldset> 
    
    </form>


  
<%- include __footer %>